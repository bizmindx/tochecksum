{
  "name": "tochecksum",
  "version": "1.0.3",
  "description": "convert ethereum address to checksum address",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "babel src/index.js --out-file dist/index.js",
    "launch": "npm run build && npm run start",
    "start": "node build/index.js",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix --ext .js ",
    "test": "jest --verbose --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codaelux/tochecksum.git"
  },
  "keywords": [
    "ethereum",
    "checksum address",
    "tochecksum address",
    "checksum converter",
    "extended public key",
    "ethereum public key"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codaelux/tochecksum/issues"
  },
  "homepage": "https://github.com/codaelux/tochecksum/blob/master/README.md",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "jest": "^25.2.3"
  },
  "dependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "keccak": "^3.0.0"
  }
}
